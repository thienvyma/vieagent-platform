{
  "name": "vieagent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "start": "NODE_ENV=production next start --experimental-https",
    "start:dev": "next start",
    "start:https": "next start --experimental-https",
    "start:prod": "NODE_ENV=production next start --experimental-https",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:production": "vercel --prod",
    "deploy:preview": "vercel",
    "vercel-build": "npm run build",
    "verify:production": "node scripts/production-deployment-verification.js",
    "verify:deployment": "echo 'ðŸš€ Vercel Deployment Ready - All blockers resolved!' && npm run verify:webhooks",
    "test:auth-flows": "node scripts/test-authentication-flows.js",
    "test:platforms": "node scripts/test-platform-deployment.js",
    "verify:webhooks": "node scripts/verify-webhook-deployment.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "node scripts/migrate-and-seed.js",
    "db:reset": "prisma db push --force-reset",
    "setup:chromadb": "chmod +x scripts/setup-chromadb.sh && ./scripts/setup-chromadb.sh",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:vercel": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh --target vercel",
    "deploy:gcp": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh --target gcp-vm",
    "deploy:supabase": "node scripts/deploy-supabase-schema.js",
    "postinstall": "prisma generate",
    "email:test": "node scripts/test-email.js",
    "health": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "create-admin": "tsx scripts/create-admin.ts",
    "create-owner": "tsx scripts/create-owner.ts",
    "test:db": "tsx scripts/test-db.ts",
    "test:subscriptions": "tsx scripts/test-subscriptions-plans.ts",
    "test:blog": "tsx scripts/test-blog-announcements.ts",
    "dev:full": "npm run db:generate && npm run dev",
    "backup:system": "tsx scripts/backup-system.ts",
    "restore:backup": "tsx scripts/restore-backup.ts",
    "test:chromadb": "tsx scripts/test-chromadb-integration.ts",
    "test:embedding": "tsx scripts/test-embedding-service.ts",
    "test:agent-config": "tsx scripts/test-agent-config.ts",
    "test:day4-original": "tsx scripts/day4-foundation-test-original.ts",
    "test:day4-fallback": "tsx scripts/day4-foundation-test-fallback.ts",
    "chromadb:start": "python scripts/start-chromadb-simple.py",
    "chromadb:start-advanced": "python scripts/start-chromadb-server.py",
    "test:day30": "node scripts/run-day30-tests.js",
    "test:day30-integration": "node scripts/day30-integration-testing.js",
    "test:day30-performance": "node scripts/day30-performance-benchmarking.js",
    "test:day30-load": "node scripts/day30-load-testing.js",
    "test:day30-security": "node scripts/day30-security-auditing.js",
    "monitor": "node scripts/monitor-free-tier.js",
    "backup": "node scripts/backup-free-tier.js",
    "health-check": "curl -f http://localhost:3001/api/health || exit 1",
    "monitor:cron": "node -e \"const cron = require('cron'); const job = new cron.CronJob('*/5 * * * *', () => { require('./scripts/monitor-free-tier.js'); }); job.start(); console.log('Monitoring cron started');\"",
    "backup:daily": "node -e \"const cron = require('cron'); const job = new cron.CronJob('0 2 * * *', () => { require('./scripts/backup-free-tier.js'); }); job.start(); console.log('Backup cron started');\""
  },
